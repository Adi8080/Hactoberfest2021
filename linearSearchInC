// linear search

#include <stdio.h>
int main()
{
    int arr[5] = {3,5,11,5,4}, match;
    int valToMatch;
    printf("enter the value to find : ");
    scanf("%d", &valToMatch);
    
    for(int i=0; i<5; i++)
    {
        if(valToMatch == arr[i])
        {
            printf("value %d at %d", valToMatch, i+1);
            break;
        }
        else
        {
            printf("not found");
        }
    }
}
